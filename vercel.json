{
  "version": 2,
  "functions": {
    "api/**": {
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "headers": [
    {
      "source": "/(.*)\\.(css|js|svg|png|jpg|jpeg|gif|webp|ico|woff|woff2)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
